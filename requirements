# SQL Queries:
CREATE TABLE IF NOT EXISTS dogs (id INT AUTO_INCREMENT PRIMARY KEY, name VARCHAR(255), breed VARCHAR(255), gender VARCHAR(255), age VARCHAR(255), size VARCHAR(255), city VARCHAR(255), inquiries INT)
CREATE TABLE IF NOT EXISTS adoptions (id INT AUTO_INCREMENT PRIMARY KEY, dog_id INT,  FOREIGN KEY (dog_id) REFERENCES dogs(id),first_name VARCHAR(255),last_name VARCHAR(255),email VARCHAR(255),phone VARCHAR(255))